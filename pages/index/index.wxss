page{
  height: 100%;
  background: #b3d4db;
}
.container{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.user_logo{
  width: 200rpx;
  height: 200rpx;
  margin-top: 160rpx;
  margin-bottom: 20rpx;
  border-radius: 50%;
  background-color: #fff;
  font-size: 22rpx;
  text-align: center;
  line-height: 200rpx;
}
.user_name{
  font-size: 32rpx;
  font-weight: bold;
  color: #405f80;
}
.container_btn{
  height: 80rpx;
  width: 200rpx;
  margin-top: 200rpx;
  border: 1px solid #405f80;
  border-radius: 5px;
  text-align: center;
}
.btn_text{
  font-size: 22rpx;
  font-weight: bold;
  line-height: 80rpx;
  color: #405f80;
}